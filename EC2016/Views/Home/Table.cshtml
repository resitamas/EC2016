@model List<EC2016.Models.TeamModel>

<table class="black-text" border="1" style="margin: 0 auto; text-align:center">
    <tr>
        <th style="text-align:center">H</th>
        <th style="text-align:center">Ország</th>
        <th style="text-align:center">M</th>
        <th style="text-align:center">GY</th>
        <th style="text-align:center">D</th>
        <th style="text-align:center">V</th>
        <th style="text-align:center">L</th>
        <th style="text-align:center">K</th>
        <th style="text-align:center">P</th>
    </tr>
@foreach (var item in Model)
{
    
    <tr>
        <td width="15px">@item.Position</td>
        <td width="110px">@item.Name</td>
        <td width="15px">@item.Count</td>
        <td width="15px">@item.Win</td>
        <td width="15px">@item.Draw</td>
        <td width="15px">@item.Loose</td>
        <td width="25px">@item.Scored</td>
        <td width="25px">@item.Get</td>
        <td width="25px">@item.Points</td>
    </tr>
}
</table>

