@model List<EC2016.Models.TeamModel>

<table class="black-text" border="1" style="margin: 0 auto;">
    <tr>
        <th>H</th>
        <th>Ország</th>
        <th>M</th>
        <th>GY</th>
        <th>D</th>
        <th>V</th>
        <th>L</th>
        <th>K</th>
        <th>P</th>
    </tr>
@foreach (var item in Model)
{
    
    <tr>
        <td width="15px">@item.Position</td>
        <td width="110px">@item.Name</td>
        <td width="15px">@item.Count</td>
        <td width="15px">@item.Win</td>
        <td width="15px">@item.Draw</td>
        <td width="15px">@item.Loose</td>
        <td width="25px">@item.Scored</td>
        <td width="25px">@item.Get</td>
        <td width="25px">@item.Points</td>
    </tr>
}
</table>

